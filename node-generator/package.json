{
  "scripts": {
    "start": "npm run generate && ts-node example.ts",
    "build": "rimraf ./build && npm run generate && tsc -p tsconfig.build.json && copyfiles ./generated/styles/styles.css ./build",
    "generate": "npm run generate-schema && npm run generate-sass",
    "generate-schema": "json2ts mac-schema.json generated/mac.d.ts --no-additionalProperties",
    "generate-sass": "rimraf ./generated/styles && sass --style=compressed ./sass:./generated/styles",
    "format": "prettier ./**/* --write",
    "lint": "eslint ./"
  },
  "devDependencies": {
    "@types/marked": "4.0.8",
    "copyfiles": "2.4.1",
    "eslint": "^8.36.0",
    "prettier": "^2.8.4",
    "rimraf": "4.4.0",
    "sass": "1.59.3",
    "ts-node": "^10.9.1",
    "typescript": "5.0.2"
  },
  "dependencies": {
    "json-schema-to-typescript": "12.0.0",
    "marked": "4.2.12"
  }
}
